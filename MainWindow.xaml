<Window
    x:Class="CompInfoUI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CompInfoUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Name="Das" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <ContentDialog x:Name="joinbeta" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
           PrimaryButtonText="Join" PrimaryButtonClick="joinbeta_PrimaryButtonClick" IsPrimaryButtonEnabled="False"
                       DefaultButton="Primary" RequestedTheme="Default"
           CloseButtonText="Cancel" >
            <ContentDialog.TitleTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets://CompInfoBetaIcon.png" Width="40" Height="40" Margin="0, 0, 0,0"/>
                        <TextBlock Text="CompInfo Beta" Margin ="10, 0, 0, 0"/>
                    </StackPanel>
                </DataTemplate>
            </ContentDialog.TitleTemplate>
            <StackPanel>
                <TextBlock TextWrapping="WrapWholeWords" Text="Try out beta versions of CompInfo Beta with new and upcoming features and providing your feedback will help us improve this app. Beta Builds might be unstable and buggy">
                </TextBlock>
                <CheckBox x:Name="ConfirmAgeCheckBox" Content="I accept receiving email notifications for newly released Beta Builds"
              Checked="ConfirmAgeCheckBox_Checked" Margin="0, 10, 0, 0 " Unchecked="ConfirmAgeCheckBox_Unchecked"/>
            </StackPanel>
        </ContentDialog>
        <NavigationView x:Name="nvSample"  IsSettingsVisible="True"
    IsBackButtonVisible="Collapsed"
 IsBackEnabled="True"
                        PaneTitle="CompInfo Preview"
                        Visibility="Collapsed"
                        BackRequested="nvSample_BackRequested"
                        ItemInvoked="nvSample_ItemInvoked"
    SelectionChanged="nvSample_SelectionChanged"
    Header="CompInfo"
    AlwaysShowHeader="True"
    PaneDisplayMode="LeftCompact"
    ExpandedModeThresholdWidth="500"
    SelectionFollowsFocus="Disabled"
    IsTabStop="False">
            <NavigationView.AutoSuggestBox>
                <AutoSuggestBox Name="SearchNavig" QuerySubmitted="AutoSuggestBox_QuerySubmitted" PlaceholderText="Search for PC info..." SuggestionChosen="AutoSuggestBox_SuggestionChosen" TextChanged="AutoSuggestBox_TextChanged" QueryIcon="Find"  AutomationProperties.Name="Search" />
            </NavigationView.AutoSuggestBox>
            <NavigationView.MenuItems>
                <NavigationViewItem Name="SysSum" Content="Computer Summary" Tag="syssum">
                    <NavigationViewItem.Icon>
                        <FontIcon Glyph="&#xE8FD;"/>
                    </NavigationViewItem.Icon>
                </NavigationViewItem>
                <NavigationViewItem Name="SysHard" Content="Hardware Info" Tag="hard">
                    <NavigationViewItem.Icon>
                        <FontIcon Glyph="&#xE977;"/>
                    </NavigationViewItem.Icon>
                    <NavigationViewItem.MenuItems>
                        <NavigationViewItem Name="HardCPU" Content="CPU" ToolTipService.ToolTip="Processor Info" Tag="cpuinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE950;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardGPU" Content="GPU" ToolTipService.ToolTip="Display and Graphics Info" Tag="gpuinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE7FC;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardRAM" Content="RAM" ToolTipService.ToolTip="Random Access Memory Info" Tag="raminfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE964;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardInputDevices" Content="Input Devices" ToolTipService.ToolTip="Keyboard and Mouse Info" Tag="idinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE961;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardStorageDevices" Content="Storage Devices" ToolTipService.ToolTip="Storage Info" Tag="sdinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE7F1;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardNetwork" Content="Network" ToolTipService.ToolTip="Network Info" Tag="netinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE968;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardPrinting" Content="Printing" ToolTipService.ToolTip="Printer Info" Tag="printinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE749;"/>
                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
                        <NavigationViewItem Name="HardSound" Content="Sound" ToolTipService.ToolTip="Sound Info" Tag="printinfo">
                            <NavigationViewItem.Icon>
                                <FontIcon Glyph="&#xE995;" />

                            </NavigationViewItem.Icon>
                        </NavigationViewItem>
